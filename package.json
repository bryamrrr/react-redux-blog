{
  "name": "react-redux-blog",
  "version": "0.1.0",
  "description": "Sample project using React and Redux",
  "main": "./build/server/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "bff": "node ./build/server/index.js",
    "deploy": "now",
    "deploy:sfs": "now ./build/statics/",
    "start": "npm run bff",
    "sfs": "serve ./build/statics --port 3020 --cache 0",
    "watch": "webpack -w",
    "test": "jest",
    "test:fix": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryamrrr/react-redux-blog.git"
  },
  "author": "Bryam Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bryamrrr/react-redux-blog/issues"
  },
  "homepage": "https://github.com/bryamrrr/react-redux-blog#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "15.4.2",
    "react-intl": "^2.2.3",
    "react-lazy-image": "^1.1.0",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0-beta.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-immutable": "^3.1.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "list": "^0.9.5",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "/__mocks__/styleMock.js"
    },
    "notify": true
  }
}
